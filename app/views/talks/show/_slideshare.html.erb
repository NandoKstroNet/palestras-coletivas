<% if Rails.env != 'test' %>
  <% unless @talk.code.empty? %>
    <iframe src="http://www.slideshare.net/slideshow/embed_code/<%= @talk.code %>" width="100%" height="356" frameborder="0" marginwidth="0" marginheight="0" scrolling="no" style="border:1px solid #CCC;border-width:1px 1px 0;margin-bottom:5px" allowfullscreen webkitallowfullscreen mozallowfullscreen> </iframe>
  <% else %>
    <br/>
    <h4><p class="text-warning"><%= t("show.talk.without_presentation") %></p></h4>
  <% end %>
<% end %>